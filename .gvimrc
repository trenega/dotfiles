" .gvimrc

" Font setting
set guifont=Ricty\ Diminished\ Discord\ Regular:h24

" Colorscheme setting
" Reference: https://qiita.com/shinichinomura/items/d786f1d84d1713b168e8
"            https://github.com/w0ng/vim-hybrid.git
set background=dark
"let g:hybrid_custom_term_colors = 1
"colorscheme hybrid
colorscheme iceberg

" Window size setting
set lines=42
set columns=100
